                    console.log(model);
                    $.ajax({
                        url: '/Login/Login',
                        data: model,
                        type: 'POST',
                        cache: false,
                        datatype: "json",
                        success: function (returnData) {
                            window.location = "Home/Index";
                        }
                    });
                
            
            
            })
